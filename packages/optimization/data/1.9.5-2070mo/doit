#!/bin/sh

WHICH=1.9.5-2041
OUT=boot-$WHICH
for n in 1; do
	for p in `seq 1 $n`; do
	    ../csv2svg.pl \
		-1 \
		-p $p/$n \
		-t 1 boot-power-$WHICH.csv \
		-t 2 boot-kernel-byhand-$WHICH.csv \
		-o 2.040 \
		-l 20 -t 3 boot-kernel-$WHICH.csv \
		-l 20 -t 4 boot-init-$WHICH.csv \
		-l  0 -t 5 boot-am-$WHICH.csv \
		-l  0 -t 6 boot-flash-$WHICH.csv \
		> $OUT-${p}of$n.svg
	done
done
