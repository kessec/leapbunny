killall app AppManager
cd /usr/bin
fileget mb
fileget new-calib
fileget ts-rawtest
fileput /var/log/new-calib.log
fileput /flags/set-ts.sh
fileput /flags/pointercal
mb
chmod +x ts-rawtest mfgmode.sh
mfgdata get sn
echo ++++++++++ 4 Point ++++++++++
for i in $(seq 1 4); do
      new-calib -S -x -m && fileput /var/log/new-calib.log
done
echo ++++++++++ 5 Point ++++++++++
for i in $(seq 1 3); do
      new-calib -S -x -m -5 -w && fileput /var/log/new-calib.log
done
mb
for i in /LF/Bulk/mb_*; do fileput $i; done
ts-rawtest -g
# poweroff