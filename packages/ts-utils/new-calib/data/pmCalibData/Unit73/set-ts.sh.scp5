#!/bin/sh
SYS=/sys/devices/platform/lf1000-touchscreen
# TSP Version=3
echo 28 > $SYS/max_tnt_down
#2 echo 31 > $SYS/max_tnt_down
echo 520 > $SYS/min_tnt_up
echo 20 > $SYS/max_delta_tnt
echo 5 > $SYS/delay_in_us
echo 5 > $SYS/y_delay_in_us
echo 5 > $SYS/tnt_delay_in_us
echo 425 433 470 486 520 553 619 669 751 > $SYS/pressure_curve
echo 1 > $SYS/tnt_mode
echo -1 > $SYS/averaging
echo -670 0 349600 > $SYS/tnt_plane
#2 echo 0 0 0 > $SYS/tnt_plane
